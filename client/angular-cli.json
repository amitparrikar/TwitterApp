{
  "project": {
    "version": "1.0.0-beta.16",
    "name": "twitter-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": "assets",
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "twt",
      "mobile": false,
      "styles": [
        "styles.css",
        "./assets/css/custom-app.css"
      ],
      "scripts": [
        "./assets/lib/material.min.js",
        "./assets/lib/jquery-1.12.4.min.js",
        "./assets/lib/jquery-ui.min.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false
  }
